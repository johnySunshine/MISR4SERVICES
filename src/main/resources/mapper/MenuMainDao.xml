<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soecode.lyf.dao.MenuMainDao">
    <!-- 目的：为dao接口方法提供sql语句配置 -->
    <select id="queryMainMenus" resultType="Menu">
        <!-- 具体的sql -->
        SELECT * FROM tmenus
    </select>

    <insert id="insertMainMenu">
        <!-- ignore 主键冲突，报错 -->
        INSERT ignore INTO tmenus (menutext, menuurl,target,subid)
        VALUES (#{menuText}, #{menuUrl},#{target},#{subid})
    </insert>

</mapper>