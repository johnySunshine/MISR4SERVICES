<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.msir.dao.LocationDao">
    <insert id="saveLocation">
        INSERT
        ignore
        INTO
        hot_city_location
        (n,count,pinyin_full,pinyin_short,gmt_create,gmt_modified)
        VALUES
        (#{n},#{count},#{pinyinFull},#{pinyinShort},CURRENT_TIME,CURRENT_TIME)
    </insert>
</mapper>